<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from LineChartHelperService WSDL service -->

  <service name="LineChartHelper" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://charthelper.diy.com"/>
      <parameter name="wsdlServiceElement" value="LineChartHelperService"/>
      <parameter name="schemaQualified" value="http://charthelper.diy.com"/>
      <parameter name="wsdlServicePort" value="LineChartHelper"/>
      <parameter name="className" value="com.diy.charthelper.LineChartHelperSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="LineChartHelper"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="generateWeeklyReviewLineChart" qname="operNS:generateWeeklyReviewLineChart" xmlns:operNS="http://charthelper.diy.com" returnQName="retNS:generateWeeklyReviewLineChartReturn" xmlns:retNS="http://charthelper.diy.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:IndexValues" xmlns:pns="http://charthelper.diy.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:strLastDay" xmlns:pns="http://charthelper.diy.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="generateWeeklyReviewLineChart"/>

  </service>
</deployment>
